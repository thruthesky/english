  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "s": "sudo ng serve --hmr -e=hmr --ssl true --ssl-key './tmp/privkey.pem' --ssl-cert './tmp/fullchain.pem' --host englishfordevelopers.com --port 443",
    "sa": "sudo ng serve --aot --ssl true --ssl-key './tmp/privkey.pem' --ssl-cert './tmp/fullchain.pem' --host englishfordevelopers.com --port 443",
    "ssl": "ng serve --ssl --ssl-key ./tmp/privkey.pem --ssl-cert ./tmp/fullchain.pem --host englishfordevelopers.com --port 443",
    "t": "ng build --prod --aot --output-path=www --base-href=/ && cd www && scp -r * talkative@x.withcenter.com:./www",
    "ret": "ng build --prod --output-path=www --base-href=/ && cd www && scp index.html *.css *.js talkative@x.withcenter.com:./www",
    "publish": "ng build --prod --aot --output-path=www --base-href=/ && npm run scp",
    "republish": "ng build --prod --output-path=www --base-href=/ && cd www && scp index.html *.css *.js englishfordevelopers@www.englishfordevelopers.com:./www",
    "scp": "cd www && scp -r * englishfordevelopers@www.englishfordevelopers.com:./www"
  },
  "private": true,
